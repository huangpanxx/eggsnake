package com.maple.eggsnake.desktop;

import com.badlogic.gdx.backends.jogl.JoglApplication;
import com.maple.eggsnake.application.EggSnakeApplicationListener;
import com.maple.eggsnake.logger.DefaultLogger;

public class DesktopGameStarter {
	public static void main(String args[]) {
		DefaultLogger.getInstace().Log("Enter from desktop");
		new JoglApplication(EggSnakeApplicationListener.getInstance(), "EggSnake", 480, 320, true);
	}
}
