package com.maple.eggsnake;

import com.badlogic.gdx.ApplicationListener;

public class EggSnake implements ApplicationListener {

	private static EggSnake instance = null;

	public static EggSnake getInstance() {
		synchronized (instance) {
			if (instance == null)
				instance = new EggSnake();
		}
		return instance;
	}

	private EggSnake() {
	}

	@Override
